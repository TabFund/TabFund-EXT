{
  "manifest_version": 2,
  "name": "TabFund",
  "version": "1.0.1",
  "description": "Raise money for Research while browsing the internet with each new open tab!",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png",
      "128":"images/icon128.png"
    },
    "default_title": "TabFund"
  },
  "author": "TabFund Team",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}